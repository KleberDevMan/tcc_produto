<aside class="sidenav-main nav-expanded nav-lock nav-collapsible sidenav-light sidenav-active-square">
  <div class="brand-sidebar">
    <h1 class="logo-wrapper logo-crop">
      <a class="brand-logo darken-1" href="/">
        <img class="hide-on-med-and-down" src="/woffice-beta.svg" alt="materialize logo" />
        <img class="show-on-medium-and-down hide-on-med-and-up" src="/wofficebw-w.svg" alt="materialize logo" />
      </a>
      <a class="navbar-toggler" href="#"><i class="material-icons">radio_button_checked</i></a>
    </h1>
  </div>
  <ul class="sidenav sidenav-collapsible leftside-navigation collapsible sidenav-fixed menu-shadow" id="slide-out" data-menu="menu-navigation" data-collapsible="menu-accordion">
    <ul id="slide-out" class="side-nav fixed sidenav-company leftside-navigation">
      <div class="company-sidemenu-logo-top">
        <%= image_tag("/settings/cached_logo.png?v=#{@last_logo_update}") %><span><%= company_name %></span>
      </div>
      <li class="no-padding">
        <ul class="collapsible" data-collapsible="accordion">
          <% current_user.menus.where(active: true).order(position: :asc).distinct.roots.map{|a| a.subtree.where(id: current_user.menu_ids).arrange_serializable}.each do |m| %>
              <%= render partial: "layouts/menu_tree",  locals: {m: m[0]}, cached: true %>
          <% end if current_user.present?  %>
        </ul>
      </li>
    </ul>

  </ul>
  <div class="navigation-background"></div><a class="sidenav-trigger btn-sidenav-toggle btn-floating btn-medium waves-effect waves-light hide-on-large-only" href="#" data-target="slide-out"><i class="material-icons">menu</i></a>
</aside>